<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xml2sql.xsl"?>
<project name="Excalibur" version="Phoenix 7.0" spec="https://code.kamibu.com/wiki/Phoenix/User">
	<database name="cclive" type="mysql">
		<table name="universities">
			<field name="uni_id" type="name" />
			<field name="uni_name" type="varchar" length="511" />
			<field name="uni_typeid" type="int" />
			<field name="uni_placeid" type="int" ref="places.place_id" />
			<field name="uni_created" type="datetime" />
			<field name="uni_delid" type="int" />
			<index type="primary">
				<indexfield>uni_id</indexfield>
			</index>
			<index type="index" name="LOCATION">
				<indexfield>uni_placeid</indexfield>
			</index>
		</table>
	</database>
</project>
