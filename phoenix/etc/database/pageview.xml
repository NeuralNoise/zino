<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xml2sql.xsl"?>
<project name="Excalibur" version="Phoenix 7.0">
    <database name="zinophoenix" type="mysql">
        <table name="pageviews">
            <field name="pageview_id" type="int" autoincrement="yes" />
            <field name="pageview_element" type="varchar" size="256" />
            <field name="pageview_created" type="datetime" />
            <field name="pageview_userip" type="int" />
            <field name="pageview_sessionid" type="char" size="32" />
            <index type="primary">
                <indexfield>pageview_id</indexfield>
            </index>
        </table>
    </database>
</project>
